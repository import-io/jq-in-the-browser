{
  "name": "@import/jq-in-the-browser",
  "version": "3.0.0",
  "description": "jq-compatible query language for the web",
  "main": "lib/jq-in-the-browser.js",
  "scripts": {
    "test": "mocha-webpack \"src/test.js\" \"src/common_tests.js\"  --webpack-config webpack.test.config.js",
    "test:travis": "mocha-webpack \"src/travis_test.js\" \"src/common_tests.js\"  --webpack-config webpack.test.config.js",
    "test:watch": "mocha-webpack --watch \"src/test.js\" \"src/common_tests.js\" --webpack-config webpack.test.config.js --reporter dot",
    "build": "webpack --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/import-io/jq-in-the-browser.git"
  },
  "author": "Dániel Kántor",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "jq-web": "^0.1.5",
    "json-loader": "^0.5.7",
    "mocha": "^6.2.3",
    "mocha-webpack": "^1.1.0",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.6",
    "semantic-release": "^17.1.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^3.12.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "node 10",
          "include": [
            "@babel/plugin-proposal-object-rest-spread"
          ]
        }
      ]
    ]
  }
}
